{
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/(.*\\.(?:js|css|png|jpg|jpeg|gif|svg|ico|json|txt|webmanifest|woff2?))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
